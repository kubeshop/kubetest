apiVersion: executor.testkube.io/v1
kind: Executor
metadata:
  name: {{ .Name }}
  namespace: {{ .Namespace }}
  labels:
  {{- range $key, $value := .Labels }}
    {{ $key }}: {{ $value }}
  {{- end }}
spec:
  types:
  {{- range .Types }}
  - {{ . }}
  {{- end }}  
  {{- if .ExecutorType }}
  executor_type: {{ .ExecutorType }}
  {{- end }}
  {{- if .Uri }}
  uri: {{ .Uri }}
  {{- end }}
  {{- if .Image }}
  image: {{ .Image }}
  {{- end }}
  {{- if .JobTemplate }}
  job_template: {{ .JobTemplate }}
  {{- end }}
