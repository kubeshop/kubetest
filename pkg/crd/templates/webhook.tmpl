apiVersion: executor.testkube.io/v1
kind: Webhook
metadata:
  name: {{ .Name }}
  namespace: {{ .Namespace }}
  labels:
  {{- range $key, $value := .Labels }}
    {{ $key }}: {{ $value }}
  {{- end }}
spec:
  events:
  {{- range .Events }}
  - {{ . }}
  {{- end }}
  {{- if .Uri }}
  uri: {{ .Uri }}
  {{- end }}
  {{- if .Selector }}
  selector: {{ .Selector }}
  {{- end }}
