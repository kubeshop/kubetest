vars.put(&quot;MATOMO_ID&quot;, id);
String timestamp = &quot;&quot; + System.currentTimeMillis()/1000;
vars.put(&quot;TIMESTAMP&quot;, timestamp);
String hours = String.format(&quot;%2d&quot;,${__time(H)});
vars.put(&quot;HOURS&quot;, hours);
String minutes = &quot;&quot; + ${__time(mm)};
vars.put(&quot;MINUTES&quot;, minutes);
String seconds = &quot;&quot; + ${__time(ss)};
vars.put(&quot;SECONDS&quot;, seconds);</stringProp>
